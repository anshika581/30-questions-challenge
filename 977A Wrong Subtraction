import java.util.*;
import java.lang.*;
import java.io.*;
 
public class anuj581
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
        int k = sc.nextInt();

        for (int i = 0; i < k; ++i) {
        if (n % 10 == 0) n /= 10;
        else n--;
        }
        System.out.print(n);
	}
}		
